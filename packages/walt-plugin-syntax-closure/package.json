{
  "name": "walt-plugin-syntax-closure",
  "version": "0.1.0",
  "description": "Walt Syntax Closures",
  "main": "dist/walt-plugin-syntax-closures.js",
  "scripts": {
    "test": "ava",
    "debug-container": "node --inspect-brk=\"$(hostname -I | xargs)\" node_modules/ava/profile.js",
    "build": "cross-env node_modules/rollup/bin/rollup -c rollup.config.js",
    "build-prod": "cross-env NODE_ENV=production node_modules/rollup/bin/rollup -c rollup.config.js",
    "preversion": "npm run build && npm run build-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballercat/walt.git"
  },
  "keywords": [
    "wasm",
    "wast",
    "javascript",
    "webassembly",
    "compiler"
  ],
  "author": "Arthur Buldauskas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ballercat/walt/issues"
  },
  "homepage": "https://github.com/ballercat/walt#readme",
  "dependencies": {
    "walt-parser-tools": "^0.1.1",
    "walt-syntax": "^0.1.1"
  },
  "devDependencies": {
    "ava": "0.24.0",
    "babel-cli": "6.26.0",
    "babel-eslint": "^8.1.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-register": "^6.26.0",
    "cross-env": "5.1.3",
    "eslint": "^5.0.0",
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "pirates": "^3.0.2",
    "prettier": "1.9.2",
    "rollup": "0.52.2",
    "uglify-es": "3.0.17",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-eslint": "4.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-plugin-walt-grammar": "^0.1.0",
    "walt-compiler": "^0.10.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "proseWrap": "always",
    "singleQuote": true
  },
  "ava": {
    "files": [
      "src/**/*[sS]pec.js"
    ],
    "source": [
      "src/**/*.js",
      "src/**/*.walt",
      "src/**/*.ne"
    ],
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "../../_register.js"
    ],
    "babel": "inherit"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
