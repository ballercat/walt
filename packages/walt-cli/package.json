{
  "name": "walt-cli",
  "version": "0.1.0",
  "description": "CLI for Walt",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "debug": "node --inspect-brk node_modules/ava/profile.js"
  },
  "keywords": [
    "walt",
    "wasm",
    "webassembly",
    "cli",
    "compiler"
  ],
  "author": "Arthur Buldauskas",
  "license": "MIT",
  "dependencies": {
    "meow": "^4.0.0",
    "walt-compiler": "^0.4.3",
    "walt-link": "^0.1.6",
    "walt-buildtools": "^0.0.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "prettier": "^1.11.1"
  },
  "ava": {
    "files": [
      "src/**/*[sS]pec.js",
      "!**/utils.js"
    ],
    "source": [
      "src/**/*.js",
      "src/**/*.walt"
    ],
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false
  }
}
