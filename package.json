{
  "name": "walt",
  "version": "0.2.1",
  "description": "Alternative syntax for WebAssembly text format",
  "main": "index.js",
  "scripts": {
    "test": "nyc ava",
    "tdd": "ava",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "debug": "node --inspect-brk node_modules/ava/profile.js",
    "build": "node_modules/rollup/bin/rollup -c rollup.config.js && cp dist/walt.js docs/dist/",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballercat/walt.git"
  },
  "keywords": [
    "wasm",
    "wast"
  ],
  "author": "Arthur Buldauskas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ballercat/walt/issues"
  },
  "homepage": "https://github.com/ballercat/walt#readme",
  "dependencies": {
    "codemirror": "^5.29.0",
    "curry": "^1.2.0",
    "invariant": "^2.2.2",
    "ramda": "^0.23.0",
    "react-codemirror": "^1.0.0",
    "wasm-types": "1.0.2"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "flow-bin": "^0.54.0",
    "nyc": "^11.1.0",
    "prettier": "^1.7.0",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.0.17"
  },
  "ava": {
    "files": [
      "src/**/*[sS]pec.js",
      "!**/utils.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 10,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
